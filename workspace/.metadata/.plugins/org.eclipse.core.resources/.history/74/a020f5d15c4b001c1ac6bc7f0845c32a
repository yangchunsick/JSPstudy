	//  String ip = request.getRemoteAddr();
		String ip = request.getHeader("X-Forwarded-For"); // 다른 곳 통해서 접속
		if(ip == null) {
			ip = request.getRemoteAddr(); // 직접 접속
		}
		
		// Free DTO 생성
		Free
		
		return null;
	}

}
